class Main {
    public static void main(String[] args) {

        long a = 1234567890123456789L;
        long b = 1234567890123456789L;

        while (b != 0) {
            long carry = (a & b) << 1;
            a = a ^ b;
            b = carry;
        }

        System.out.println("Sum = " + a);
    }
}
