class Main {
    public static void main(String[] args){
        Scanner in = new Scanner(System.in);
        System.out.println("enter the digits");
       int num1 = in.nextInt();
       int num2 = in.nextInt();
       int num3 = in.nextInt();
       int num4 = in.nextInt();
       System.out.println((char)num1+ " " +(char)num2+ " " +(char)num3+ " "+(char)num4+ " " );
    }
}
